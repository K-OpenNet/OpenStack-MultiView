
public class ReflectiveFieldFeeder {
	// Dummy value objects for class type info 
	private static Long lValue = 0L;
	private static Double lfValue = 0.0;	
	
	public static int addField(org.influxdb.dto.Point.Builder pointBuilder, Class dataTypeClass, Object data) {
		if (dataTypeClass.equals(lValue.getClass())) {
			builder.addField("value", (long)dataObj.get("data"));
		} else if (dataTypeClass.equals(lfValue.getClass())) {
			builder.addField("value", (double)dataObj.get("data"));			
		}				
	}
}
