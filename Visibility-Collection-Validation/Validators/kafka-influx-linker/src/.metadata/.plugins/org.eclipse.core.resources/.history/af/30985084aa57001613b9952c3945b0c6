package chainlinker;

/*
 * Corresponding to snap-plugin-collector-df
 * 
 * CAUTION: This setting may fail in case if the plugins' version mismatch with the below.
 * - collector:df:2
 */
public class SnapDFParser extends SnapPluginParser {
	public SnapDFParser() {
		super();		
		// All these data forms must be updated when snap publisher's output format is changed.
		
		// Pattern: /intel/procfs/filesystem/(Any strings without /)/(space_free or space_reserved or inodes_percent_free or inodes_percent_used or device_name or inodes_used)
		regexTypeMap.put("^\\/intel\\/procfs\\/filesystem\\/(0|[1-9][0-9]*|all)\\/utilization_percentage$", lfClass);
		
		regexSet = regexTypeMap.keySet();
	}			
	
}
